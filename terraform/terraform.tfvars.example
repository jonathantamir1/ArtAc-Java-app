# Copy this file to terraform.tfvars and fill in your values

# AWS Configuration
aws_region = "us-east-1"
instance_type = "t3.small"

# Key Pair (must exist in AWS - create this in AWS Console first)
key_pair_name = "jt-private-key"

# Docker Configuration
docker_image = "jamir7/chatbot-app"
docker_tag = "latest"

# Chatbot API Key (required)
chatbot_api_key = "your-gemini-api-key-here"
